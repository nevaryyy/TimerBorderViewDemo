<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TimerBorderView">
        <attr name="borderWidth" format="dimension"/>
        <attr name="borderColor" format="color"/>
        <attr name="startPos" format="enum">
            <enum name="topLeft" value="0" />
            <enum name="topRight" value="1" />
            <enum name="bottomLeft" value="2" />
            <enum name="bottomRight" value="3" />
        </attr>
        <attr name="clockwise" format="boolean" />
    </declare-styleable>
</resources>